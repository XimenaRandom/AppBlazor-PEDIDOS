@page "/empleado"
@inject NavigationManager navigationManager

<h3>Empleado y Venta</h3>
<button @onclick="navegar1" class="btn btn-primary">Navegar a Lista de Empleados</button>
<button @onclick="navegar2" class="btn btn-warning">Navegar a Formulario Ventas</button>
@code {
    private void navegar1()
    {
        navigationManager.NavigateTo("/formempleado");
    }
    private void navegar2()
    {
        navigationManager.NavigateTo("/formventa");
    }
}